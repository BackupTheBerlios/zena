<%= form_remote_tag(:with=>'parent', :url=>{:controller=>'node', :action=>'move', :id=>@node[:id], :rnd=>rnd}) %>
  <li id='parent_errors' class='errors'></li>
  <li><label><%= trans('parent') %></label><%= select_id('node', 'parent_id', :class=>@node.class.parent_class) %></li>
  <li><label><%= trans('name') %></label><%= text_field('node', 'name', :size=>15) %></li>
  <li class='submit'><%= submit_tag transb('change') %></li>
<%= end_form_tag %>
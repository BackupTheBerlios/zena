<ul class='drive' id='parent'>
<%= form_tag(:controller=>'item', :action=>'move', :rnd=>rnd) %>
<%= hidden_field('item', 'id' ) %>
  <li><label><%= trans('parent') %></label><%= select_id('item', 'parent_id') %></li>
  <li><label><%= trans('name') %></label><%= text_field('item', 'name', :size=>15) %></li>
  <li class='submit'><%= submit_tag transb('change') %></li>
<%= end_form_tag %>
</ul>